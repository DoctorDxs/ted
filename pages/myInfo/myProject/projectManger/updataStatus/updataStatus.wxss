.imgList image{height: 130rpx;width: 130rpx;margin: 0 20rpx;}
.addImg{border: 1px dashed #e3e4e5;height: 130rpx;width: 130rpx;font-size:130rpx;color: #e3e4e5;text-align: center;line-height: 120rpx; }
.imgList {position: relative;}
.icon-close {position:absolute;font-size: 36rpx;background: rgba(000,000,000,.5);color: #fff;right: 25rpx;top: 5rpx;}
